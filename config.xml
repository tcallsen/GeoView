<?xml version='1.0' encoding='utf-8'?>
<widget id="co.stample" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    
    <name>GeoView</name>
    <description>
        View geo data on a movile device
    </description>
    <author email="taylor.callsen@gmail.com" href="http://gritto.net">
        Taylor Callsen
    </author>
    <content src="index.html" />
